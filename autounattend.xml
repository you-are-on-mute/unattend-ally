			<UserAccounts>
				<LocalAccounts>
					<LocalAccount wcm:action="add">
						<Password>
							<Value>ChangeMeBase64==</Value>
							<PlainText>false - change me to true if using plain text password</PlainText>
						</Password>
						<DisplayName>Change Me</DisplayName>
						<Group>Administrators</Group>
						<Name>ChangeMe</Name>
					</LocalAccount>
